{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["maintenance", "chore", "dependencies"]
    },
    {
      "title": "## âš¡ Performance",
      "labels": ["performance"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## ğŸ—ï¸ Infrastructure",
      "labels": ["infrastructure", "ci", "docker"]
    }
  ],
  "ignore_labels": [
    "ignore-for-release"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "#{{TITLE}} by @#{{AUTHOR}} in ##{{NUMBER}}",
  "pr_template": "- #{{TITLE}} by @#{{AUTHOR}} in ##{{NUMBER}}",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\(.+\\))?:",
      "target": "$1"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[backport \\w+\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "^(feat|feature):",
      "target": "feature"
    },
    {
      "pattern": "^(fix|bugfix):",
      "target": "bug"
    },
    {
      "pattern": "^docs?:",
      "target": "documentation"
    },
    {
      "pattern": "^(chore|build|ci):",
      "target": "maintenance"
    },
    {
      "pattern": "^perf:",
      "target": "performance"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "base_branches": [
    "main"
  ]
}
